#!/usr/bin/env bash

# Enable debug mode
if [[ "$DEBUG" ]]; then
  export PS4='+ [${BASH_SOURCE##*/}:$LINENO] '
  set -x
fi

set -euo pipefail

export PATH="$HOME/.dotfiles/share/syno/tasks:$PATH"

# This comes last inside the Synology Task so it pulls the HC for this main
# script
# static-healthchecks

static-dhcp-leases
static-openvpn
static-opnsense
static-opnsense-unbound
static-pihole
static-prowlarr
static-speedtest
static-ssl-check
static-syno
static-transmission
static-vnstat
